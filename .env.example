# ============================================================
# kk_nl2sql_aibot 环境变量配置
# 复制此文件为 .env 并填写实际值
# cp .env.example .env
# ============================================================

# --- PostgreSQL ---
POSTGRES_USER=kk_nl2sql
POSTGRES_PASSWORD=kk_nl2sql_secret_2026
POSTGRES_DB=kk_nl2sql
POSTGRES_PORT=5432

# --- Redis ---
REDIS_PORT=6379

# --- Milvus ---
MILVUS_PORT=19530
MILVUS_METRIC_PORT=9091

# --- Attu (Milvus 可视化) ---
ATTU_PORT=8000

# --- MinIO (业务文件存储) ---
MINIO_PORT=9000
MINIO_CONSOLE_PORT=9001
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=admin123456

# --- MemOS Cloud (记忆系统 - 在线服务) ---
MEMOS_API_URL=https://api.openmem.net
MEMOS_API_KEY=your-memos-api-key

# --- Celery ---
CELERY_BROKER_URL=redis://redis:6379/1
CELERY_RESULT_BACKEND=redis://redis:6379/2

# --- 数据管理 ---
MAX_UPLOAD_SIZE_MB=100
ALLOWED_UPLOAD_TYPES=xlsx,csv,sqlite,xls

# --- SQL 安全 ---
SQL_EXECUTION_TIMEOUT=30
SQL_MAX_RESULT_ROWS=1000

# --- LLM API Keys (Phase 3+) ---
# DEEPSEEK_API_KEY=sk-xxx
# QWEN_API_KEY=sk-xxx

# --- JWT (Phase 3+) ---
# JWT_SECRET_KEY=your-secret-key-change-me
# JWT_ALGORITHM=HS256
