# --- PostgreSQL ---
POSTGRES_USER=kk_gpt
POSTGRES_PASSWORD=kk_gpt_secret_2026
POSTGRES_DB=kk_gpt
POSTGRES_PORT=5432

# --- Redis ---
REDIS_PORT=6379

# --- Milvus ---
MILVUS_PORT=19530
MILVUS_METRIC_PORT=9091

# --- Attu (Milvus 可视化) ---
ATTU_PORT=8000

# --- MinIO (业务文件存储) ---
MINIO_PORT=9000
MINIO_CONSOLE_PORT=9001
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=admin123456

# --- MemOS Cloud (记忆系统 - 在线服务) ---
MEMOS_API_URL=https://api.openmem.net
MEMOS_API_KEY=your-memos-api-key

